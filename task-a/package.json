{
  "name": "backend",
  "version": "1.0.0",
  "author": "Sivan Wolberg",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main",
    "webpack": "webpack --config webpack.config.js --progress",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:schema:drop": "npm run typeorm schema:drop",
    "docs:dev": "vuepress dev -p 7070",
    "docs:build": "cross-env DEPLOY_ENV=gh-pages vuepress build",
    "docs:deploy": "npm run docs:build && gh-pages -d .vuepress/dist"
  },
  "dependencies": {
    "@casl/ability": "^4.1.6",
    "@hapi/joi": "^17.1.1",
    "@mobizerg/nest-bull": "^0.1.3",
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/microservices": "^7.5.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.0",
    "bull": "^3.20.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "compress": "^0.99.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "ioredis": "^4.19.4",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "mysql": "^2.18.1",
    "nest-event": "^1.0.8",
    "nest-logger": "^6.1.0",
    "nestjs-command": "^1.5.3",
    "nestjs-logzio-logger": "^5.7.2",
    "nestjs-pino": "^1.3.0",
    "pino-multi-stream": "^5.1.1",
    "pino-papertrail": "^2.1.0",
    "redis": "^3.0.2",
    "redis-url": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "request-context": "^2.0.0",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^2.1.3",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29",
    "typeorm-transactional-cls-hooked": "^0.1.12",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.14.6",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.11",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "faker": "^5.1.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "jest": "^26.6.3",
    "lint-staged": "~9.4.2",
    "nodemon": "^1.19.4",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typeorm-factories": "^1.0.1",
    "typeorm-fixtures-cli": "^1.8.1",
    "typescript": "^4.0.5",
    "vuepress": "^1.5.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  }
}
